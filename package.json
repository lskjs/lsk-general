{
  "version": "2.0.0",
  "name": "lskjs",
  "description": "LSK.js â€“ command line interface helper",
  "author": "Igor Suvorov <hi@isuvorov.com>",
  "license": "MIT",
  "repository": "https://github.com/lskjs/lskjs",
  "keywords": ["lskjs", "cli", "helper"],
  "scripts": {
    "commit": "./scripts/run/commit.sh",
    "start": "./scripts/start.sh",
    "dev": "./scripts/dev.sh",
    "dev:cra-and-server": "lerna exec -- npm run dev:cra-and-server",
    "dev:cra": "lerna exec -- npm run dev:cra",
    "dev:server": "lerna exec -- npm run dev:server",
    "release": "lerna publish --skip-npm --exact && npm run release:after",
    "release:yes": "lerna publish --skip-npm --exact --github-release --yes && npm run release:after",
    "release:beta": "lerna publish --skip-npm --exact --github-release --yes --conventional-prerelease && npm run release:after",
    "release:after": "./scripts/release-after.sh",
    "release:it": "release-it",
    "release:changelog": "lerna-changelog",
    "release:storybook": "storybook-to-ghpages",
    "i18": "./scripts/i18.sh",
    "bootstrap": "./scripts/bootstrap.sh",
    "linkwatch": "./scripts/linkwatch-all.sh",
    "update-starter-kit": "./scripts/pdate-starter-kit/check-unstaged-files.sh && ./scripts/update-starter-kit/run.sh",
    "update": "lerna exec -- npm run update",
    "update:force": "lerna exec -- ../../scripts/package/update.sh",
    "audit": "lerna exec --parallel --no-bail -- npm audit",
    "build": "./scripts/run/build.sh",
    "watch": "lerna run --parallel watch",
    "bump": "lerna run --parallel bump",
    "storybook": "./scripts/run/storybook.sh",
    "pre-commit": "echo npm run lint:staged",
    "pre-push": "echo npm run lint:prettier",
    "prettier": "prettier",
    "lint": "npm run lint:js",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint . --ext .js --fix && prettier --write --plugin-search-dir=.",
    "lint:prettier": "prettier --list-different \"./**/*.js\"",
    "lint:js": "lerna exec -- npm run lint:js",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "test:coverage": "jest --coverage --verbose --color",
    "test:jest": "jest --verbose --color",
    "test": "npm run lint && npm run test:jest-coverage",
    "contributors:generate": "node_modules/.bin/all-contributors generate"
  },
  "dependencies": {
    "@babel/cli": "7.12.13",
    "@babel/core": "7.12.13",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-proposal-decorators": "7.12.13",
    "@babel/plugin-proposal-export-namespace-from": "7.12.13",
    "@babel/plugin-proposal-function-bind": "7.12.13",
    "@babel/plugin-proposal-optional-chaining": "7.12.13",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.12.13",
    "@babel/plugin-transform-runtime": "7.12.15",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@babel/runtime": "7.12.13",
    "@emotion/babel-plugin": "11.1.2",
    "@release-it/conventional-changelog": "2.0.0",
    "@types/config": "0.0.38",
    "@types/jest": "26.0.20",
    "@types/react": "17.0.1",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-root-import": "6.6.0",
    "babel-plugin-transform-class-property-arrow-to-bind": "1.1.0",
    "babel-preset-minify": "0.5.1",
    "commitizen": "4.2.3",
    "concurrently": "5.3.0",
    "lerna": "3.22.1",
    "lint-staged": "10.5.4",
    "react": "17.0.1",
    "shelljs": "0.8.4",
    "ts-interface-builder": "0.2.3",
    "typescript": "4.1.3",
    "webpack": "5.21.1",
    "webpack-dev-server": "3.11.2"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@commitlint/prompt": "11.0.0",
    "@lskjs/build-locales": "1.98.0",
    "@lskjs/utils": "2.2.0-beta.20",
    "all-contributors-cli": "6.19.0",
    "babel-eslint": "10.1.0",
    "cz-conventional-changelog": "3.3.0",
    "cz-customizable": "6.3.0",
    "eslint": "7.19.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lerna-changelog": "1.0.1",
    "nodemon": "2.0.7",
    "npm-check-updates": "11.1.1",
    "prettier": "2.2.1",
    "release-it": "14.3.0",
    "release-it-lerna-changelog": "3.1.0",
    "size-limit": "4.9.2",
    "ts-node-dev": "1.1.1"
  },
  "config": {
    "cz-customizable": {
      "config": ".czrc.js"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
