{
  "version": "0.1.4",
  "description": "Lego starter kit command line helper",
  "author": "Igor Suvorov <hi@isuvorov.com>",
  "license": "MIT",
  "repository": "https://github.com/isuvorov/lib-starter-kit",
  "keywords": [],
  "scripts": {
    "commit": "./node_modules/cz-customizable/standalone.js -S",
    "start": "lerna exec -- npm run start",
    "dev": "./scripts/dev.sh",
    "dev:cra-and-server": "lerna exec -- npm run dev:cra-and-server",
    "dev:cra": "lerna exec -- npm run dev:cra",
    "dev:server": "lerna exec -- npm run dev:server",
    "release": "lerna publish --skip-npm --exact && npm run release:after",
    "release:yes": "lerna publish --skip-npm --exact --github-release --yes && npm run release:after",
    "release:after": "./scripts/release-after.sh",
    "release:it": "release-it",
    "release:changelog": "lerna-changelog",
    "release:storybook": "storybook-to-ghpages",
    "build-locales": "./scripts/build-locales.sh",
    "i18": "./scripts/i18.sh",
    "bootstrap": "./scripts/bootstrap.sh",
    "dns": "./scripts/dns.sh",
    "linkwatch": "./scripts/linkwatch.sh",
    "clear": "./scripts/clear.sh",
    "update-starter-kit": "./scripts/check-unstaged-files.sh && ./scripts/update-starter-kit.sh",
    "update": "lerna exec -- npm run update",
    "update-force": "lerna exec -- ../../scripts/package-update.sh",
    "audit": "lerna exec --parallel --no-bail -- npm audit",
    "build": "./scripts/build.sh",
    "watch": "lerna run --parallel watch",
    "link": "lerna exec -- npm run link",
    "bump": "lerna exec -- npm run bump",
    "storybook": "./scripts/storybook.sh",
    "pre-commit": "echo npm run lint:staged",
    "pre-push": "echo npm run lint:prettier",
    "prettier": "prettier",
    "lint": "npm run lint:js && npm run lint:md && npm run lint:spellcheck",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint . --ext .js --fix && prettier --write --plugin-search-dir=.",
    "lint:prettier": "prettier --list-different \"./**/*.js\"",
    "lint:js": "lerna exec -- npm run lint:js",
    "lint:md": "remark -q .",
    "lint:spellcheck": "./scripts/spellcheck.sh",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "test:coverage": "jest --coverage --verbose --color",
    "test:jest": "jest --verbose --color",
    "test": "npm run lint && npm run test:jest-coverage",
    "pretty-staged": "pretty-quick --staged",
    "contributors:add": "node_modules/.bin/all-contributors add",
    "contributors:generate": "node_modules/.bin/all-contributors generate"
  },
  "dependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "7.8.3",
    "@babel/plugin-proposal-function-bind": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.9.6",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-stage-0": "7.8.3",
    "@babel/preset-typescript": "7.9.0",
    "@babel/runtime": "7.9.6",
    "@types/react": "16.9.35",
    "@types/storybook__react": "5.2.1",
    "@storybook/storybook-deployer": "2.8.5",
    "babel-loader": "8.1.0",
    "babel-plugin-emotion": "10.0.33",
    "babel-plugin-root-import": "6.5.0",
    "babel-plugin-transform-class-property-arrow-to-bind": "1.1.0",
    "babel-preset-minify": "0.5.1",
    "lerna": "3.21.0",
    "react": "16.8.6",
    "react-scripts": "3.4.1",
    "shelljs": "0.8.4",
    "typescript": "3.9.2",
    "webpack": "4.42.0",
    "webpack-dev-server": "3.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@commitlint/prompt": "8.3.5",
    "@lskjs/build-locales": "1.21.0",
    "@lskjs/linkall": "1.20.0",
    "@lskjs/log": "1.20.0",
    "@release-it/conventional-changelog": "1.1.4",
    "@storybook/react": "5.3.18",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "all-contributors-cli": "6.16.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "24.9.0",
    "commitizen": "4.1.2",
    "company-commit": "1.1.1",
    "concurrently": "5.2.0",
    "cross-env": "7.0.2",
    "cz-conventional-changelog": "3.2.0",
    "cz-customizable": "6.2.0",
    "dnsjack": "1.0.0",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-babel-plugin-root-import": "1.1.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "glob": "7.1.6",
    "husky": "4.2.5",
    "jest": "24.9.0",
    "lerna-changelog": "1.0.1",
    "lint-staged": "10.2.2",
    "nodemon": "2.0.4",
    "npm-check-updates": "5.0.0",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "react-styleguidist": "11.0.6",
    "react-test-renderer": "16.13.1",
    "release-it": "13.6.0",
    "release-it-lerna-changelog": "2.3.0",
    "remark": "12.0.0",
    "remark-cli": "8.0.0",
    "remark-preset-lint-recommended": "4.0.0",
    "size-limit": "4.5.0",
    "storybox": "1.1.0",
    "yaspeller": "7.0.0"
  },
  "config": {
    "cz-customizable": {
      "config": ".czrc.js"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
